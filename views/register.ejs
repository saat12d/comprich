    <div class="">
      <div class="container">
        <div class="row">
          <div class="col-lg-7 ml-auto mr-auto">
            <div class="card card-register">
              <h3 class="title mx-auto" style="color: white">Welcome</h3>
              <form class="register-form" action="/register" method="POST" enctype="multipart/form-data">
                  <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="fname">First Name</label>
                        <input class="form-control" type="text" id="fname" name="user[firstName]" placeholder="First Name">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="lname">Last Name</label>
                        <input class="form-control" type="text" id="lname" name="user[lastName]" placeholder="Last Name">
                    </div>
                
                    <div class="form-group col-md-6">
                        <label>Email</label>
                        <input type="text" class="form-control" name="user[email]" placeholder="Email">     
                    </div>
                    <div class="form-group col-md-6">
                        <label for="pfp">Profile Picture</label>
                        <input type="file" name="image" class="form-control-file" accept="image/*">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="school">Which school are you from?</label>
                        <select id="school" class="form-control" aria-placeholder="School" name="user[school]">
                            <option value="gma">Gems Modern Academy</option>
                            <option value="dia">Dubai Internation Academy</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label for="year">Which year/grade are you in?</label>
                        <select id="year" class="form-control" name="user[yearLevel]">
                            <% for(let i = 1; i < 13; i++){ %>
                                <option value="<%= i + 1 %>">Year <%= i + 1 %> / Grade <%= i %></option>
                            <% } %>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label for="username">Username</label>
                        <input type="text" class="form-control" name="username" placeholder="Username">
                    </div>
                    <div class="form-group col-md-6">
                        <label>Password</label>
                        <input type="password" class="form-control" name="password" placeholder="Password">
                    </div>
                    <div id="acForm" class="form-group col-md-6 d-flex align-items-center justify-content-center">
                      <button id="acButton" class="btn btn-info btn-block btn-round">Are you an Admin?</button>
                    </div>
                    <div id="acDisplay" class="form-group col-md-6">
                      <label for="ac">Admin Code</label>
                      <input type="text" class="form-control" placeholder="Admin Code" name="adminCode">
                    </div>
                    <button class="btn btn-danger btn-block btn-round">Register</button>
                  </div>
              </form>
              <div class="forgot">
                <a href="#" class="btn btn-link btn-danger">Forgot password?</a>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class=" text-center">
        <h6>Â©
          <script>
