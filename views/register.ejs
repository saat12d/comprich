    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script>
      $(document).ready(() => {
        $('#acDisplay').hide();
        $('#acButton').click(e => {
          e.preventDefault();
          $('#acDisplay').toggle();
        })
      })
    </script>
